<?xml version="1.0" encoding="utf-8"?>
<launch>

 <arg name="eval" default="false"/>

<group if="$(arg eval)">
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find cfear_radarodometry)/rviz/odom_eval.rviz" />
</group>
<group unless="$(arg eval)">
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find cfear_radarodometry)/rviz/odom.rviz" />
</group>

</launch>
